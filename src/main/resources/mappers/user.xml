<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="LacunaMatata.Lacuna.repository.UserMapper">

    <select id="findByUserId" resultType="LacunaMatata.Lacuna.entity.User">
        select
            *
        from
            tb_user
        where
            user_id = #{userId}
    </select>
</mapper>